{
  "name": "quisquamquibusdam",
  "version": "1.0.0",
  "keywords": [
    "quia",
    "tslint",
    "inventore",
    "autem",
    "config",
    "typescript"
  ],
  "files": [
    "src/index.js"
  ],
  "main": "src/index.js",
  "author": "BradlyKemmer",
  "repository": {
    "type": "git",
    "url": "https://github.com/KevonParisian/quisquamquibusdam.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.1",
    "jest": "^20.0.1",
    "lodash": "^4.17.4",
    "prettier": "^1.4.2",
    "semantic-release": "^6.3.6",
    "tslint": "^5.4.3",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4",
    "validate-commit-msg": "^2.10.1"
  },
  "bugs": {
    "url": "https://github.com/KevonParisian/quisquamquibusdam.git/issues"
  }
}